  },
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf ./dist && mkdir dist",
    "prepublish": "yarn build",
    "build_browser": "NODE_ENV=production TARGET=web webpack",
    "build_server": "NODE_ENV=production TARGET=node webpack",
    "build": "yarn build_browser && yarn build_server",
    "dev": "microbundle -w src/index.js",
    "pretest": "yarn build_browser",
    "test": "nyc mocha --timeout 30000 -R spec --compilers js:@babel/register test/*.spec.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "baobab": "^2.5.2",
    "superagent": "^4.0.0",
    "superagent-prefix": "^0.0.2",
    "superagent-suffix": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-airbnb": "^3.0.1",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.6.1",
    "istanbul": "^0.4.5",
    "jsdom": "^12.2.0",
    "mocha": "^5.2.0",
    "mocha-clean": "^1.0.0",
